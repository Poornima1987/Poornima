
//string palindrome using recursion
#include<iostream>
using namespace std;
int main()
{
char name[100];
cin >> name;
int i=0, j=strlen(name)-1;
bool X= true;
for(i=0 ; i<j; i++);
{
if(name[i]!=name[j]) {
X= false;
}
j--;
}
if(X) {
cout << “ The string is palindrome \n”;
}
else {
cout << “ no \n”;
}
return 0;
}

