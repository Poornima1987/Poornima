#include <bits/std++.h>
using namespace std;
int MaxSubarrayXOR (int arr[],int n)
{
int ans = INT_MIN;
for (int i =0; i < n;i++)
{
int curr_XOR = 0;
for (int j=I;j<n;j++)
{
curr_XOR = curr_XOR^arr[j];
ans=max(ans,curr_XOR);
}
}
return ans;
}
int main()
{
int arr[] = {8,1,2,12};
int n= sizeof(arr)/sizeof(arr[0]);
cout<<”Max sub array XOR is”<<maxSubarray XOR(arr,n0;
return 0;
}

